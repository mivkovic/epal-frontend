{
  "name": "epal-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node prepare-env.js && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean:dist": "rm -rf ./dist",
    "dev:ssr": "node prepare-env.js && ng run epal-frontend:serve-ssr",
    "serve:ssr": "node dist/epal-frontend/server/main.js",
    "build:ssr": "node prepare-env.js && ng build --prod && ng run epal-frontend:server:production",
    "prerender": "ng run epal-frontend:prerender",
    "check": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "pre-push": "npm run check",
      "pre-add": "npm run check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/platform-server": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@datorama/akita": "^4.22.2",
    "@nguniversal/express-engine": "^9.1.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "husky": "^4.2.5",
    "lodash": "^4.17.15",
    "mobile-detect": "^1.4.4",
    "redis": "^3.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nguniversal/builders": "^9.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.35",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
